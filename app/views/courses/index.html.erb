<% provide(:title, t(".title")) %>

<section class="courses_area p_120">
  <div class="container">
    <div class="main_title">
      <h2><%= t".heading" %></h2>
    </div>
    <%= render "search_form" %>
    <div class="row courses_inner">
      <div class="col-lg-9 course">
        <div class="grid_inner">
          <%= render @courses %>
        </div>
      </div>
    </div>
    <p><%= will_paginate @courses, class: "apple_pagination" %></p>
  </div>
</section>
