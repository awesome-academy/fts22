<% provide(:title, t(".title")) %>

<section class="courses_area p_120">
  <div class="container">
    <div class="section-top-border">
      <h3 class="mb-30 title_color"><%= t".heading_trainer" %></h3>
      <%= link_to t(".new_trainee"), new_trainer_trainee_path, class: "btn btn-primary" %>
      <div class="progress-table-wrap">
        <div class="progress-table" id="task_table">
          <div class="table-head">
            <div class="country"><%= t".name" %></div>
            <div class="country"><%= t".email" %></div>
            <div class="country"><%= t".created_at" %></div>
            <div class="country"><%= t".procedure" %></div>
          </div>
          <%= render partial: "trainee", collection: @trainees, as: :trainee %>
          <p><%= will_paginate @trainees, class: "apple_pagination" %></p>
        </div>
      </div>
    </div>
  </div>
</section>
